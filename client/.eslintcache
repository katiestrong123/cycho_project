.\" \n                        onChange={handleFilterInput} \n                        type=\"text\" \n                        id=\"cycho-input\"/>\n                </div>\n            <div className=\"location-selector\">   \n                <select onChange={handleLocationSelect}\n                        name=\"location-search-type\"\n                        id=\"location-filter-select\">\n                    <option value=\"choose\">Choose location</option>\n                    <option value=\"cenrtral\">Central London</option>\n                    <option value=\"North\">North London</option>\n                    <option value=\"East\">East London</option>\n                    <option value=\"South\">South London</option>\n                    <option value=\"West\">West London</option>\n                </select>\n            </div>\n        </div>\n    </>\n    )\n\n};\n\nexport default CychoFilterForm;","/Users/katiestrong/cycho/client/src/components/AboutCycho.js",[],["112","113"],"/Users/katiestrong/cycho/client/src/components/Footer.js",[],"/Users/katiestrong/cycho/client/src/store.js",[],["114","115"],"/Users/katiestrong/cycho/client/src/reducers/index.js",[],"/Users/katiestrong/cycho/client/src/components/chat/Chat.js",[],"/Users/katiestrong/cycho/client/src/actions/types.js",[],"/Users/katiestrong/cycho/client/src/actions/watson.js",[],"/Users/katiestrong/cycho/client/src/reducers/watson.js",["116"],"//import types \nimport {INPUT_SUCCESS, INPUT_FAIL,\n    SESSION_SUCCESS, SESSION_FAIL, MESSAGE_SUCCESS, MESSAGE_FAIL\n} from \"../actions/types\";\n\n//initial state \nconst initialState = {\n    messages:[],\n};\n\n//switch statement - update success\n\nexport default (state=initialState, action) => {\n    const {type, payload} =action; \n    let {messages} = state;\n\n    switch(type){\n        case INPUT_SUCCESS:\n            messages = [...messages, {message: payload, type: \"user\"}]\n            return {\n                ...state,\n                messages,\n            };\n            case INPUT_FAIL:\n                return {\n                    ...state,\n            };\n            case SESSION_SUCCESS:\n                localStorage.setItem(\"session\", payload[\"session_id\"])\n                return {\n                    ...state,\n                } \n                case SESSION_FAIL:\n                return {\n                    ...state,\n                }\n                case MESSAGE_SUCCESS:\n                    messages = [...messages, {message:payload, type:\"bot\"}]\n                    return {\n                        ...state,\n                        messages,\n                    }\n                case MESSAGE_FAIL:\n                    return {\n                        ...state,\n                    }\n            default:\n                return {\n                    ...state,\n                };\n    }\n};",{"ruleId":"117","replacedBy":"118"},{"ruleId":"119","replacedBy":"120"},{"ruleId":"121","severity":1,"message":"122","line":3,"column":8,"nodeType":"123","messageId":"124","endLine":3,"endColumn":13},{"ruleId":"121","severity":1,"message":"125","line":5,"column":8,"nodeType":"123","messageId":"124","endLine":5,"endColumn":16},{"ruleId":"121","severity":1,"message":"126","line":6,"column":8,"nodeType":"123","messageId":"124","endLine":6,"endColumn":23},{"ruleId":"121","severity":1,"message":"127","line":7,"column":8,"nodeType":"123","messageId":"124","endLine":7,"endColumn":21},{"ruleId":"128","severity":1,"message":"129","line":27,"column":57,"nodeType":"130","messageId":"131","endLine":27,"endColumn":59},{"ruleId":"128","severity":1,"message":"129","line":47,"column":58,"nodeType":"130","messageId":"131","endLine":47,"endColumn":60},{"ruleId":"132","severity":1,"message":"133","line":64,"column":13,"nodeType":"134","endLine":64,"endColumn":35},{"ruleId":"121","severity":1,"message":"135","line":3,"column":8,"nodeType":"123","messageId":"124","endLine":3,"endColumn":17},{"ruleId":"121